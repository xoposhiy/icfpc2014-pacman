LDF sampleList
AP 0
LDC 3
LDF get
AP 2
RTN
sampleList: ; fun()
  LDC 1
  LDC 2
  LDC 3
  LDC 4
  LDC 0
  CONS
  CONS
  CONS
  CONS
  RTN
;end sampleList

get: ; fun(list, index)
  LD 0 1  ; index
  SEL nonzero_2 zero_1
    LDC 0
    TSEL endif_3 endif_3
    nonzero_2:
      LD 0 0  ; list
      CDR
      LD 0 1  ; index
      LDC 1
      Sub
      LDF get
      AP 2
      JOIN
    zero_1:
      LD 0 0  ; list
      Car
      JOIN
    endif_3:
  RTN
;end get

